{
  "version": 2,
  "builds": [
    {
      "src": "bazi_api.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/bazi_api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}